{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nexport default {\n  data() {\n    return {\n      products: [{\n        id: 1,\n        name: \"Coombes\",\n        price: 9600,\n        category: \"LOUNGE\",\n        stars: 5,\n        image: \"Coombes.png\"\n      }, {\n        id: 2,\n        name: \"Keev Set\",\n        price: 1200,\n        category: \"TABLE & CHAIRS\",\n        stars: 5,\n        image: \"Keev_Set.png\"\n      }, {\n        id: 3,\n        name: \"Nillè\",\n        price: 2950,\n        category: \"ARMCHAIRS\",\n        stars: 2,\n        image: \"Nillè.png\"\n      }, {\n        id: 4,\n        name: \"Blanko\",\n        price: 1090,\n        category: \"SIDE TABLE\",\n        stars: 4,\n        image: \"Blanko.png\"\n      }, {\n        id: 5,\n        name: \"Momo\",\n        price: 2250,\n        category: \"SHELVES\",\n        stars: 1,\n        image: \"Momo.png\"\n      }, {\n        id: 6,\n        name: \"Penemillè\",\n        price: 1200,\n        category: \"CHAIR\",\n        stars: 4,\n        image: \"Penemillè.png\"\n      }, {\n        id: 7,\n        name: \"Kappu\",\n        price: 1020,\n        category: \"SHELVES\",\n        stars: 3,\n        image: \"kappu.png\"\n      }],\n      cart: [],\n      cartVisible: false,\n      searchQuery: \"\",\n      sortOrder: \"\",\n      filteredProducts: []\n    };\n  },\n  mounted() {\n    this.filteredProducts = this.products;\n  },\n  methods: {\n    getImage(imageName) {\n      return require(`../assets/images/${imageName}`);\n    },\n    addToCart(product) {\n      const existingProduct = this.cart.find(item => item.id === product.id);\n      if (existingProduct) {\n        existingProduct.quantity++;\n      } else {\n        this.cart.push({\n          ...product,\n          quantity: 1\n        });\n      }\n    },\n    toggleCart() {\n      this.cartVisible = !this.cartVisible;\n    },\n    filterProducts() {\n      const query = this.searchQuery.toLowerCase();\n      this.filteredProducts = this.products.filter(product => product.name.toLowerCase().includes(query) || product.category.toLowerCase().includes(query));\n      this.sortProducts();\n    },\n    sortProducts() {\n      if (this.sortOrder === \"asc\") {\n        this.filteredProducts.sort((a, b) => a.price - b.price);\n      } else if (this.sortOrder === \"desc\") {\n        this.filteredProducts.sort((a, b) => b.price - a.price);\n      } else if (this.sortOrder === \"stars\") {\n        this.filteredProducts.sort((a, b) => b.stars - a.stars);\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","products","id","name","price","category","stars","image","cart","cartVisible","searchQuery","sortOrder","filteredProducts","mounted","methods","getImage","imageName","require","addToCart","product","existingProduct","find","item","quantity","push","toggleCart","filterProducts","query","toLowerCase","filter","includes","sortProducts","sort","a","b"],"sources":["/home/kapopay/Copy_Shop/src/components/product_list.vue"],"sourcesContent":["<template>\n  <header>\n    <div class=\"header_container\">\n      <div id=\"logo\">\n        <a href=\"#\" id=\"logo_link\">\n          <img src=\"../assets/images/icon.png\" alt=\"Logo\" />\n        </a>\n      </div>\n      <nav id=\"menus\">\n        <a href=\"/\">Home</a>\n        <a href=\"/shop\">Shop</a>\n        <a href=\"/magazine\">Magazine</a>\n      </nav>\n      <div id=\"cart_login\">\n        <div id=\"cart_summary\" @click=\"toggleCart\">\n          <img id=\"cart_icon\" src=\"../assets/images/panier.png\" alt=\"Cart\" />\n          <span v-if=\"cart.length > 0\">{{ cart.length }} item(s)</span>\n        </div>\n        <a href=\"/login\">LOGIN</a>\n      </div>\n    </div>\n    <!-- Affichage du panier dans une petite boîte -->\n    <div v-if=\"cartVisible\" id=\"cart_details\">\n      <h3>Panier</h3>\n      <div v-for=\"item in cart\" :key=\"item.id\" class=\"cart_item\">\n        <img :src=\"getImage(item.image)\" :alt=\"item.name\" />\n        <div class=\"cart_item_info\">\n          <h4>{{ item.name }}</h4>\n          <p>Prix: ${{ item.price }}</p>\n          <p>Quantité: {{ item.quantity }}</p>\n          <p>Total: ${{ item.price * item.quantity }}</p>\n        </div>\n      </div>\n      <div v-if=\"cart.length === 0\">\n        <p>Votre panier est vide.</p>\n      </div>\n    </div>\n  </header>\n\n  <div class=\"search_bar\">\n    <div id=\"search_container\">\n      <img id=\"search_icon\" src=\"../assets/images/loupe.png\" alt=\"Search\" />\n      <input\n        id=\"search_input\"\n        type=\"search\"\n        placeholder=\"Search\"\n        v-model=\"searchQuery\"\n        @input=\"filterProducts\"\n      />\n      <span class=\"powered_by\">Powered by Algolia</span>\n    </div>\n    <select id=\"search_filter\" v-model=\"sortOrder\" @change=\"sortProducts\">\n      <option value=\"\">Select</option>\n      <option value=\"asc\">Ascending price</option>\n      <option value=\"desc\">Descending price</option>\n      <option value=\"stars\">Sort by stars</option>\n    </select>\n  </div>\n\n  <div class=\"main_cont\">\n    <div id=\"roll\">\n      <p>Filtred by</p>\n      <select class=\"custom-select\" name=\"category\" id=\"Colections\">\n        <option value=\"\">Collection</option>\n        <option value=\"\">ah</option>\n        <option value=\"\">ah</option>\n      </select>\n\n      <select class=\"custom-select\" name=\"category\" id=\"Colors\">\n        <option value=\"\">Color</option>\n        <option value=\"\">Green</option>\n        <option value=\"\">Blue</option>\n      </select>\n\n      <select class=\"custom-select\" name=\"category\" id=\"catas\">\n        <option value=\"\">Category</option>\n        <option value=\"\">Spider</option>\n        <option value=\"\">Goldfish</option>\n      </select>\n\n      <div>\n        <p>Price Range</p>\n        <input\n          type=\"range\"\n          class=\"custom-range\"\n          id=\"price\"\n          name=\"price\"\n          min=\"0\"\n          max=\"10000\"\n          v-model=\"price\"\n          step=\"10\"\n        />\n      </div>\n    </div>\n    <div v-for=\"product in filteredProducts\" :key=\"product.id\" class=\"product\">\n      <router-link :to=\"`/products/${product.id}`\">\n        <img :src=\"getImage(product.image)\" :alt=\"product.name\" />\n      </router-link>\n      <div class=\"info\">\n        <h3>{{ product.name }}</h3>\n        <p>${{ product.price }}</p>\n        <p>{{ product.category }}</p>\n        <p>{{ \"★\".repeat(product.stars) + \"☆\".repeat(5 - product.stars) }}</p>\n      </div>\n      <div class=\"cart\">\n        <a href=\"#\" @click=\"addToCart(product)\">\n          <img\n            class=\"plus\"\n            src=\"../assets/images/plus_plus.png\"\n            alt=\"add to cart\"\n          />\n        </a>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      products: [\n        {\n          id: 1,\n          name: \"Coombes\",\n          price: 9600,\n          category: \"LOUNGE\",\n          stars: 5,\n          image: \"Coombes.png\",\n        },\n        {\n          id: 2,\n          name: \"Keev Set\",\n          price: 1200,\n          category: \"TABLE & CHAIRS\",\n          stars: 5,\n          image: \"Keev_Set.png\",\n        },\n        {\n          id: 3,\n          name: \"Nillè\",\n          price: 2950,\n          category: \"ARMCHAIRS\",\n          stars: 2,\n          image: \"Nillè.png\",\n        },\n        {\n          id: 4,\n          name: \"Blanko\",\n          price: 1090,\n          category: \"SIDE TABLE\",\n          stars: 4,\n          image: \"Blanko.png\",\n        },\n        {\n          id: 5,\n          name: \"Momo\",\n          price: 2250,\n          category: \"SHELVES\",\n          stars: 1,\n          image: \"Momo.png\",\n        },\n        {\n          id: 6,\n          name: \"Penemillè\",\n          price: 1200,\n          category: \"CHAIR\",\n          stars: 4,\n          image: \"Penemillè.png\",\n        },\n        {\n          id: 7,\n          name: \"Kappu\",\n          price: 1020,\n          category: \"SHELVES\",\n          stars: 3,\n          image: \"kappu.png\",\n        },\n      ],\n      cart: [],\n      cartVisible: false,\n      searchQuery: \"\",\n      sortOrder: \"\",\n      filteredProducts: [],\n    };\n  },\n  mounted() {\n    this.filteredProducts = this.products;\n  },\n  methods: {\n    getImage(imageName) {\n      return require(`../assets/images/${imageName}`);\n    },\n    addToCart(product) {\n      const existingProduct = this.cart.find((item) => item.id === product.id);\n      if (existingProduct) {\n        existingProduct.quantity++;\n      } else {\n        this.cart.push({ ...product, quantity: 1 });\n      }\n    },\n    toggleCart() {\n      this.cartVisible = !this.cartVisible;\n    },\n    filterProducts() {\n      const query = this.searchQuery.toLowerCase();\n      this.filteredProducts = this.products.filter(\n        (product) =>\n          product.name.toLowerCase().includes(query) ||\n          product.category.toLowerCase().includes(query)\n      );\n      this.sortProducts();\n    },\n    sortProducts() {\n      if (this.sortOrder === \"asc\") {\n        this.filteredProducts.sort((a, b) => a.price - b.price);\n      } else if (this.sortOrder === \"desc\") {\n        this.filteredProducts.sort((a, b) => b.price - a.price);\n      } else if (this.sortOrder === \"stars\") {\n        this.filteredProducts.sort((a, b) => b.stars - a.stars);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n#cart_summary {\n  cursor: pointer;\n}\n\n#cart_details {\n  position: absolute;\n  top: 60px;\n  right: 20px;\n  background-color: white;\n  padding: 15px