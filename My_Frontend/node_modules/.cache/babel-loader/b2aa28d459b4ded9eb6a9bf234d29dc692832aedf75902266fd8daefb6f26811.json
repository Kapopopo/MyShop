{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nexport default {\n  data() {\n    return {\n      products: [{\n        id: 1,\n        name: \"Coombes\",\n        price: 9600,\n        category: \"LOUNGE\",\n        stars: 5,\n        image: \"Coombes.png\"\n      }, {\n        id: 2,\n        name: \"Keev Set\",\n        price: 1200,\n        category: \"TABLE & CHAIRS\",\n        stars: 5,\n        image: \"Keev_Set.png\"\n      }, {\n        id: 3,\n        name: \"Nillè\",\n        price: 2950,\n        category: \"ARMCHAIRS\",\n        stars: 2,\n        image: \"Nillè.png\"\n      }, {\n        id: 4,\n        name: \"Blanko\",\n        price: 1090,\n        category: \"SIDE TABLE\",\n        stars: 4,\n        image: \"Blanko.png\"\n      }, {\n        id: 5,\n        name: \"Momo\",\n        price: 2250,\n        category: \"SHELVES\",\n        stars: 1,\n        image: \"Momo.png\"\n      }, {\n        id: 6,\n        name: \"Penemillè\",\n        price: 1200,\n        category: \"CHAIR\",\n        stars: 4,\n        image: \"Penemillè.png\"\n      }, {\n        id: 7,\n        name: \"Kappu\",\n        price: 1020,\n        category: \"SHELVES\",\n        stars: 3,\n        image: \"kappu.png\"\n      }],\n      cart: [],\n      cartVisible: false,\n      searchQuery: \"\",\n      sortOrder: \"\",\n      filteredProducts: [],\n      selectedStars: \"\",\n      selectedCollection: \"\",\n      price: 0\n    };\n  },\n  mounted() {\n    this.filteredProducts = [...this.products];\n  },\n  methods: {\n    getImage(imageName) {\n      return require(`../assets/images/${imageName}`);\n    },\n    addToCart(product) {\n      const existingProduct = this.cart.find(item => item.id === product.id);\n      if (existingProduct) {\n        existingProduct.quantity++;\n      } else {\n        this.cart.push({\n          ...product,\n          quantity: 1\n        });\n      }\n    },\n    toggleCart() {\n      this.cartVisible = !this.cartVisible;\n    },\n    filterProducts() {\n      let filtered = this.products;\n      if (this.searchQuery) {\n        filtered = filtered.filter(product => product.name.toLowerCase().includes(this.searchQuery.toLowerCase()) || product.category.toLowerCase().includes(this.searchQuery.toLowerCase()));\n      }\n\n      // Appliquer les autres filtres ici\n      if (this.selectedCollection) {\n        filtered = filtered.filter(product => product.category === this.selectedCollection);\n      }\n      if (this.selectedStars) {\n        filtered = filtered.filter(product => product.stars === parseInt(this.selectedStars));\n      }\n      if (this.price > 0) {\n        filtered = filtered.filter(product => product.price <= this.price);\n      }\n      this.filteredProducts = filtered;\n    },\n    sortProducts() {\n      if (this.sortOrder === \"asc\") {\n        this.filteredProducts.sort((a, b) => a.price - b.price);\n      } else if (this.sortOrder === \"desc\") {\n        this.filteredProducts.sort((a, b) => b.price - a.price);\n      } else if (this.sortOrder === \"stars\") {\n        this.filteredProducts.sort((a, b) => b.stars - a.stars);\n      }\n    },\n    filterByStars() {\n      this.filterProducts();\n    },\n    filterByCollection() {\n      this.filterProducts();\n    },\n    // Réinitialiser le filtrage complet lorsque l'utilisateur efface la sélection de collection\n    resetFilters() {\n      this.selectedCollection = \"\";\n      this.selectedStars = \"\";\n      this.searchQuery = \"\";\n      this.price = 0;\n      this.filterProducts();\n    }\n  }\n};","map":{"version":3,"names":["data","products","id","name","price","category","stars","image","cart","cartVisible","searchQuery","sortOrder","filteredProducts","selectedStars","selectedCollection","mounted","methods","getImage","imageName","require","addToCart","product","existingProduct","find","item","quantity","push","toggleCart","filterProducts","filtered","filter","toLowerCase","includes","parseInt","sortProducts","sort","a","b","filterByStars","filterByCollection","resetFilters"],"sources":["/home/kapopay/Copy_Shop/src/components/product_list.vue"],"sourcesContent":["<template>\n  <div id=\"alli\">\n    <!-- Header -->\n    <header>\n      <div class=\"header_container\">\n        <div id=\"logo\">\n          <a href=\"#\" id=\"logo_link\">\n            <img src=\"../assets/images/icon.png\" alt=\"Logo\" />\n          </a>\n        </div>\n        <nav id=\"menus\">\n          <a href=\"/\">Home</a>\n          <a href=\"/shop\">Shop</a>\n          <a href=\"/magazine\">Magazine</a>\n        </nav>\n        <div id=\"cart_login\">\n          <div id=\"cart_summary\" @click=\"toggleCart\">\n            <img id=\"cart_icon\" src=\"../assets/images/panier.png\" alt=\"Cart\" />\n            <span v-if=\"cart.length > 0\">{{ cart.length }} item(s)</span>\n          </div>\n          <a href=\"/login\">LOGIN</a>\n        </div>\n      </div>\n      <div v-if=\"cartVisible\" id=\"cart_details\">\n        <h3>Panier</h3>\n        <div v-for=\"item in cart\" :key=\"item.id\" class=\"cart_item\">\n          <img :src=\"getImage(item.image)\" :alt=\"item.name\" />\n          <div class=\"cart_item_info\">\n            <h4>{{ item.name }}</h4>\n            <p>Prix: ${{ item.price }}</p>\n            <p>Quantité: {{ item.quantity }}</p>\n            <p>Total: ${{ item.price * item.quantity }}</p>\n          </div>\n        </div>\n        <div v-if=\"cart.length === 0\">\n          <p>Votre panier est vide.</p>\n        </div>\n      </div>\n    </header>\n\n    <!-- Search and Filters -->\n    <div class=\"search_bar\">\n      <div id=\"search_container\">\n        <img id=\"search_icon\" src=\"../assets/images/loupe.png\" alt=\"Search\" />\n        <input\n          id=\"search_input\"\n          type=\"search\"\n          placeholder=\"Search\"\n          v-model=\"searchQuery\"\n          @input=\"filterProducts\"\n        />\n        <span class=\"powered_by\">Powered by Algolia</span>\n      </div>\n      <select id=\"search_filter\" v-model=\"sortOrder\" @change=\"sortProducts\">\n        <option value=\"\">Select</option>\n        <option value=\"asc\">Ascending price</option>\n        <option value=\"desc\">Descending price</option>\n        <option value=\"stars\">Sort by stars</option>\n      </select>\n    </div>\n\n    <!-- Filters Section -->\n    <div class=\"main_cont\">\n      <div id=\"roll\">\n        <p>Filtred by</p>\n\n        <!-- Collection Filter -->\n        <select\n          class=\"custom-select\"\n          name=\"category\"\n          id=\"Colections\"\n          v-model=\"selectedCollection\"\n          @change=\"filterByCollection\"\n        >\n          <option value=\"\">Collection</option>\n          <option value=\"LOUNGE\">LOUNGE</option>\n          <option value=\"CHAIRS\">CHAIRS</option>\n          <option value=\"ARMCHAIRS\">ARMCHAIRS</option>\n          <option value=\"SHELVES\">SHELVES</option>\n        </select>\n\n        <!-- Color Filter (Optional) -->\n        <select class=\"custom-select\" name=\"category\" id=\"Colors\">\n          <option value=\"\">Color</option>\n          <option value=\"Green\">Green</option>\n          <option value=\"Blue\">Blue</option>\n        </select>\n\n        <!-- Stars Filter -->\n        <select\n          class=\"custom-select\"\n          name=\"category\"\n          id=\"catas\"\n          v-model=\"selectedStars\"\n          @change=\"filterByStars\"\n        >\n          <option value=\"\">Stars</option>\n          <option value=\"5\">★★★★★</option>\n          <option value=\"4\">★★★★☆</option>\n          <option value=\"3\">★★★☆☆</option>\n          <option value=\"2\">★★☆☆☆</option>\n          <option value=\"1\">★☆☆☆☆</option>\n        </select>\n\n        <!-- Price Range Filter -->\n        <div id=\"range\">\n          <p>Price Range</p>\n          <input\n            type=\"range\"\n            class=\"custom-range\"\n            id=\"price\"\n            name=\"price\"\n            min=\"0\"\n            max=\"10000\"\n            v-model=\"price\"\n            step=\"10\"\n          />\n        </div>\n      </div>\n\n      <!-- Product Listings -->\n      <div\n        v-for=\"product in filteredProducts\"\n        :key=\"product.id\"\n        class=\"product\"\n      >\n        <router-link :to=\"`/products/${product.id}`\">\n          <img :src=\"getImage(product.image)\" :alt=\"product.name\" />\n        </router-link>\n        <div class=\"info\">\n          <p>{{ product.name }}</p>\n          <p>${{ product.price }}</p>\n          <p>{{ product.category }}</p>\n          <p>{{ \"★\".repeat(product.stars) + \"☆\".repeat(5 - product.stars) }}</p>\n        </div>\n        <div class=\"cart\">\n          <a href=\"#\" @click=\"addToCart(product)\">\n            <img\n              class=\"plus\"\n              src=\"../assets/images/plus_plus.png\"\n              alt=\"add to cart\"\n            />\n          </a>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <!-- Footer and Pagination -->\n  <footer>\n    <div class=\"pagination\">\n      <span v-for=\"page in pages\" :key=\"page\" class=\"page-item\">\n        <a class=\"page-link\" href=\"#\">{{ page }}</a>\n      </span>\n    </div>\n  </footer>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      products: [\n        {\n          id: 1,\n          name: \"Coombes\",\n          price: 9600,\n          category: \"LOUNGE\",\n          stars: 5,\n          image: \"Coombes.png\",\n        },\n        {\n          id: 2,\n          name: \"Keev Set\",\n          price: 1200,\n          category: \"TABLE & CHAIRS\",\n          stars: 5,\n          image: \"Keev_Set.png\",\n        },\n        {\n          id: 3,\n          name: \"Nillè\",\n          price: 2950,\n          category: \"ARMCHAIRS\",\n          stars: 2,\n          image: \"Nillè.png\",\n        },\n        {\n          id: 4,\n          name: \"Blanko\",\n          price: 1090,\n          category: \"SIDE TABLE\",\n          stars: 4,\n          image: \"Blanko.png\",\n        },\n        {\n          id: 5,\n          name: \"Momo\",\n          price: 2250,\n          category: \"SHELVES\",\n          stars: 1,\n          image: \"Momo.png\",\n        },\n        {\n          id: 6,\n          name: \"Penemillè\",\n          price: 1200,\n          category: \"CHAIR\",\n          stars: 4,\n          image: \"Penemillè.png\",\n        },\n        {\n          id: 7,\n          name: \"Kappu\",\n          price: 1020,\n          category: \"SHELVES\",\n          stars: 3,\n          image: \"kappu.png\",\n        },\n      ],\n      cart: [],\n      cartVisible: false,\n      searchQuery: \"\",\n      sortOrder: \"\",\n      filteredProducts: [],\n      selectedStars: \"\",\n      selectedCollection: \"\",\n      price: 0,\n    };\n  },\n  mounted() {\n    this.filteredProducts = [...this.products];\n  },\n  methods: {\n    getImage(imageName) {\n      return require(`../assets/images/${imageName}`);\n    },\n    addToCart(product) {\n      const existingProduct = this.cart.find((item) => item.id === product.id);\n      if (existingProduct) {\n        existingProduct.quantity++;\n      } else {\n        this.cart.push({ ...product, quantity: 1 });\n      }\n    },\n    toggleCart() {\n      this.cartVisible = !this.cartVisible;\n    },\n    filterProducts() {\n      let filtered = this.products;\n\n      if (this.searchQuery) {\n        filtered = filtered.filter(\n          (product) =>\n            product.name\n              .toLowerCase()\n              .includes(this.searchQuery.toLowerCase()) ||\n            product.category\n              .toLowerCase()\n              .includes(this.searchQuery.toLowerCase())\n        );\n      }\n\n      // Appliquer les autres filtres ici\n      if (this.selectedCollection) {\n        filtered = filtered.filter(\n          (product) => product.category === this.selectedCollection\n        );\n      }\n\n      if (this.selectedStars) {\n        filtered = filtered.filter(\n          (product) => product.stars === parseInt(this.selectedStars)\n        );\n      }\n\n      if (this.price > 0) {\n        filtered = filtered.filter((product) => product.price <= this.price);\n      }\n\n      this.filteredProducts = filtered;\n    },\n\n    sortProducts() {\n      if (this.sortOrder === \"asc\") {\n        this.filteredProducts.sort((a, b) => a.price - b.price);\n      } else if (this.sortOrder === \"desc\") {\n        this.filteredProducts.sort((a, b) => b.price - a.price);\n      } else if (this.sortOrder === \"stars\") {\n        this.filteredProducts.sort((a, b) => b.stars - a.stars);\n      }\n    },\n    filterByStars() {\n      this.filterProducts();\n    },\n    filterByCollection() {\n      this.filterProducts();\n    },\n    // Réinitialiser le filtrage complet lorsque l'utilisateur efface la sélection de collection\n    resetFilters() {\n      this.selectedCollection = \"\";\n      this.selectedStars = \"\";\n      this.searchQuery = \"\";\n      this.price = 0;\n      this.filterProducts();\n    },\n  },\n};\n</script>\n"],"mappings":";;;;AA+JA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QACEC,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,SAAS;QACfC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,QAAQ;QAClBC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,UAAU;QAChBC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,gBAAgB;QAC1BC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,WAAW;QACrBC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,QAAQ;QACdC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,YAAY;QACtBC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,EAAE,EAAE,CAAC