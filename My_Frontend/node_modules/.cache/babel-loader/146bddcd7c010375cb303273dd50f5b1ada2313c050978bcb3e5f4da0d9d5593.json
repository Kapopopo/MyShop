component_router_view","_component_footer_component"],"sources":["/home/kapopay/Copy_Shop/src/components/product_list.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <header_component />\n    <div>\n      <select ref=\"searchFilter\" id=\"search_filter\">\n        <option value=\"\">Select Order</option>\n        <option value=\"Ascending order\">Ascending order</option>\n        <option value=\"Descending order\">Descending order</option>\n      </select>\n      <input ref=\"searchInput\" id=\"search_input\" type=\"text\" placeholder=\"Search...\" />\n    </div>\n    <div class=\"main_cont\">\n      <!-- Vos produits ici avec la classe \"product\" et les attributs data-price et data-index -->\n    </div>\n    <router-view />\n    <footer_component />\n  </div>\n</template>\n\n<script>\nimport header_component from './components/header_component.vue';\nimport footer_component from './components/footer_component.vue';\n\nexport default {\n  components: {\n    header_component,\n    footer_component,\n  },\n  mounted() {\n    // Ajouter des écouteurs d'événements après le montage du composant\n    this.$refs.searchFilter.addEventListener(\"change\", this.handleFilterChange);\n    this.$refs.searchInput.addEventListener(\"input\", this.handleSearchInput);\n  },\n  methods: {\n    handleFilterChange() {\n      const changeOrder = this.$refs.searchFilter.value;\n      if (changeOrder === \"Ascending order\") {\n        this.prices_products(true);\n      } else if (changeOrder === \"Descending order\") {\n        this.prices_products(false);\n      } else if (changeOrder === \"\") {\n        this.reset_order();\n      }\n    },\n    prices_products(ascending) {\n      const allProducts = this.$el.querySelector(\".main_cont\");\n      const products = Array.from(allProducts.getElementsByClassName(\"product\"));\n\n      products.sort((a, b) => {\n        const price1 = parseFloat(a.getAttribute(\"data-price\"));\n        const price2 = parseFloat(b.getAttribute(\"data-price\"));\n        return ascending ? price1 - price2 : price2 - price1;\n      });\n\n      products.forEach(product => {\n        allProducts.appendChild(product);\n      });\n    },\n    reset_order() {\n      const allProducts = this.$el.querySelector(\".main_cont\");\n      const products = Array.from(allProducts.getElementsByClassName(\"product\"));\n\n      products.sort((a, b) => {\n        const index1 = parseInt(a.getAttribute(\"data-index\"));\n        const index2 = parseInt(b.getAttribute(\"data-index\"));\n        return index1 - index2;\n      });\n\n      products.forEach(product => {\n        allProducts.appendChild(product);\n      });\n    },\n    handleSearchInput() {\n      const searchAll = this.$refs.searchInput.value.toLowerCase();\n      const products = this.$el.querySelectorAll(\".product\");\n\n      products.forEach(product => {\n        const allNames = product.querySelector(\".info p\").textContent.toLowerCase();\n        product.style.display = allNames.includes(searchAll) ? \"block\" : \"none\";\n      });\n    }\n  }\n};\n</script>\n\n<style>\n/* Ajoutez ici vos styles si nécessaire */\n</style>\n"],"mappings":";;EACOA,EAAE,EAAC;AAAK;;EAGDC,GAAG,EAAC,cAAc;EAACD,EAAE,EAAC;;;EAKvBC,GAAG,EAAC,aAAa;EAACD,EAAE,EAAC,cAAc;EAACE,IAAI,EAAC,MAAM;EAACC,WAAW,EAAC;;;;;;uBARvEC,mBAAA,CAeM,OAfNC,UAeM,GAdJC,YAAA,CAAoBC,2BAAA,GACpBC,mBAAA,CAOM,cANJA,mBAAA,CAIS,UAJTC,UAIS,EAAAC,MAAA,QAAAA,MAAA,OAHPF,mBAAA,CAAsC;IAA9BG,KAAK,EAAC;EAAE,GAAC,cAAY,qBAC7BH,mBAAA,CAAwD;IAAhDG,KAAK,EAAC;EAAiB,GAAC,iBAAe,qBAC/CH,mBAAA,CAA0D;IAAlDG,KAAK,EAAC;EAAkB,GAAC,kBAAgB,oB,0BAEnDH,mBAAA,CAAiF,SAAjFI,UAAiF,8B,6BAEnFJ,mBAAA,CAEM;IAFDK,KAAK,EAAC;EAAW,IACpBC,mBAAA,2FAA4F,C,sBAE9FR,YAAA,CAAeS,sBAAA,GACfT,YAAA,CAAoBU,2BAAA,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}