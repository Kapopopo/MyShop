    },\n        {\n          id: 6,\n          name: \"Penemillè\",\n          price: 1200,\n          category: \"CHAIR\",\n          stars: 4,\n          image: \"Penemillè.png\",\n        },\n        {\n          id: 7,\n          name: \"Kappu\",\n          price: 1020,\n          category: \"SHELVES\",\n          stars: 3,\n          image: \"kappu.png\",\n        },\n      ],\n    };\n  },\n  mounted() {\n    const productId = parseInt(this.id, 10);\n    this.product = this.products.find((p) => p.id === productId);\n  },\n  methods: {\n    getImage(imageName) {\n      return require(`../assets/images/${imageName}`);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.product-details {\n  padding: 20px;\n  max-width: 800px;\n  margin: 0 auto;\n  font-family: Arial, sans-serif;\n  background-color: #f9f9f9;\n  border-radius: 10px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n}\n\n.product-title {\n  text-align: center;\n  font-size: 2rem;\n  margin-bottom: 20px;\n  color: #333;\n}\n\n.product-container {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.product-image {\n  max-width: 100%;\n  height: auto;\n  border-radius: 10px;\n  margin-bottom: 20px;\n}\n\n.product-info {\n  text-align: center;\n}\n\n.product-price {\n  font-size: 1.5rem;\n  font-weight: bold;\n  color: #e63946;\n}\n\n.product-category {\n  font-size: 1.2rem;\n  color: #555;\n  margin: 10px 0;\n}\n\n.product-rating .stars {\n  color: #ffcc00;\n  font-size: 1.2rem;\n}\n\n.product-rating .stars-empty {\n  color: #ccc;\n  font-size: 1.2rem;\n}\n\n.back-link {\n  display: inline-block;\n  margin-top: 20px;\n  text-decoration: none;\n  color: #1d3557;\n  font-weight: bold;\n  background-color: #f1faee;\n  padding: 10px 20px;\n  border-radius: 5px;\n  transition: background-color 0.3s ease;\n}\n\n.back-link:hover {\n  background-color: #a8dadc;\n}\n\n.product-not-found {\n  text-align: center;\n  padding: 20px;\n  font-size: 1.2rem;\n}\n</style>\n"],"mappings":";;AAgCA,eAAe;E