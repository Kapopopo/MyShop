uter un produit au panier\n    addToCart(product) {\n      const existingProduct = this.cart.find(item => item.id === product.id);\n      if (existingProduct) {\n        existingProduct.quantity++;\n      } else {\n        this.cart.push({\n          ...product,\n          quantity: 1\n        });\n      }\n      // Sauvegarder le panier dans le localStorage après chaque ajout\n      localStorage.setItem(\"cart\", JSON.stringify(this.cart));\n    },\n    // Afficher/Masquer le panier\n    toggleCart() {\n      this.cartVisible = !this.cartVisible;\n    },\n    // Filtrage des produits\n    filterProducts() {\n      let filtered = this.products;\n      if (this.searchQuery) {\n        filtered = filtered.filter(product => product.name.toLowerCase().includes(this.searchQuery.toLowerCase()) || product.category.toLowerCase().includes(this.searchQuery.toLowerCase()));\n      }\n      if (this.selectedCollection) {\n        filtered = filtered.filter(product => product.category === this.selectedCollection);\n      }\n      if (this.selectedStars) {\n        filtered = filtered.filter(product => product.stars === parseInt(this.selectedStars));\n      }\n      if (this.price > 0) {\n        filtered = filtered.filter(product => product.price <= this.price);\n      }\n      this.filteredProducts = filtered;\n    },\n    // Trier les produits\n    sortProducts() {\n      if (this.sortOrder === \"asc\") {\n        this.filteredProducts.sort((a, b) => a.price - b.price);\n      } else if (this.sortOrder === \"desc\") {\n        this.filteredProducts.sort((a, b) => b.price - a.price);\n      } else if (this.sortOrder === \"stars\") {\n        this.filteredProducts.sort((a, b) => b.stars - a.stars);\n      }\n    },\n    // Méthodes de filtrage par étoiles et collection\n    filterByStars() {\n      this.filterProducts();\n    },\n    filterByCollection() {\n      this.filterProducts();\n    },\n    // Méthode pour vider le panier (si nécessaire)\n    clearCart() {\n      this.cart = [];\n      localStorage.removeItem(\"cart\");\n    }\n  }\n};","map":{"version":3,"names":["data","products","id","name","price","category","stars","image","cart","cartVisible","searchQuery","sortOrder","filteredProducts","selectedStars","selectedCollection","mounted","storedCart","localStorage","getItem","JSON","parse","methods","getImage","imageName","require","addToCart","product","existingProduct","find","item","quantity","push","setItem","stringify","toggleCart","filterProducts","filtered","filter","toLowerCase","includes","parseInt","sortProducts","sort","a","b","filterByStars","filterByCollection","clearCart","removeItem"],"sources":["/home/kapopay/Copy_Shop/src/components/product_list.vue"],"sourcesContent":["<template>\n  <div id=\"alli\">\n    <!-- Header -->\n    <header>\n      <div class=\"header_container\">\n        <div id=\"logo\">\n          <a href=\"#\" id=\"logo_link\">\n            <img src=\"../assets/images/icon.png\" alt=\"Logo\" />\n          </a>\n        </div>\n        <nav id=\"menus\">\n          <a href=\"/\">Home</a>\n          <a href=\"/shop\">Shop</a>\n          <a href=\"/magazine\">Magazine</a>\n        </nav>\n        <div id=\"cart_login\">\n          <div id=\"cart_summary\" @click=\"toggleCart\">\n            <a href=\"/cart\"\n              ><img id=\"cart_icon\" src=\"../assets/images/panier.png\" alt=\"Cart\"\n            /></a>\n            <span v-if=\"cart.length > 0\">{{ cart.length }} item(s)</span>\n          </div>\n          <a href=\"/login\">LOGIN</a>\n        </div>\n      </div>\n    </header>\n\n    <!-- Product Listings -->\n    <div class=\"main_cont\">\n      <div\n        v-for=\"product in filteredProducts\"\n        :key=\"product.id\"\n        class=\"product\"\n      >\n        <router-link :to=\"`/products/${product.id}`\">\n          <img :src=\"getImage(product.image)\" :alt=\"product.name\" />\n        </router-link>\n        <div class=\"info\">\n          <p>{{ product.name }}</p>\n          <p>${{ product.price }}</p>\n          <p>{{ product.category }}</p>\n          <p>{{ \"★\".repeat(product.stars) + \"☆\".repeat(5 - product.stars) }}</p>\n        </div>\n        <div class=\"cart\">\n          <a href=\"#\" @click=\"addToCart(product)\">\n            <img\n              class=\"plus\"\n              src=\"../assets/images/plus_plus.png\"\n              alt=\"add to cart\"\n            />\n          </a>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <!-- Footer and Pagination -->\n  <footer>\n    <div class=\"pagination\">\n      <span v-for=\"page in pages\" :key=\"page\" class=\"page-item\">\n        <a class=\"page-link\" href=\"#\">{{ page }}</a>\n      </span>\n    </div>\n  </footer>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      products: [\n        {\n          id: 1,\n          name: \"Coombes\",\n          price: 9600,\n          category: \"LOUNGE\",\n          stars: 5,\n          image: \"Coombes.png\",\n        },\n        {\n          id: 2,\n          name: \"Keev Set\",\n          price: 1200,\n          category: \"TABLE & CHAIRS\",\n          stars: 5,\n          image: \"Keev_Set.png\",\n        },\n        {\n          id: 3,\n          name: \"Nillè\",\n          price: 2950,\n          category: \"ARMCHAIRS\",\n          stars: 2,\n          image: \"Nillè.png\",\n        },\n        {\n          id: 4,\n          name: \"Blanko\",\n          price: 1090,\n          category: \"SIDE TABLE\",\n          stars: 4,\n          image: \"Blanko.png\",\n        },\n        {\n          id: 5,\n          name: \"Momo\",\n          price: 2250,\n          category: \"SHELVES\",\n          stars: 1,\n          image: \"Momo.png\",\n        },\n        {\n          id: 6,\n          name: \"Penemillè\",\n          price: 1200,\n          category: \"CHAIR\",\n          stars: 4,\n          image: \"Penemillè.png\",\n        },\n        {\n          id: 7,\n          name: \"Kappu\",\n          price: 1020,\n          category: \"SHELVES\",\n          stars: 3,\n          image: \"kappu.png\",\n        },\n      ],\n      cart: [],\n      cartVisible: false,\n      searchQuery: \"\",\n      sortOrder: \"\",\n      filteredProducts: [],\n      selectedStars: \"\",\n      selectedCollection: \"\",\n      price: 0,\n    };\n  },\n  mounted() {\n    // Charger le panier depuis le localStorage\n    const storedCart = localStorage.getItem(\"cart\");\n    if (storedCart) {\n      this.cart = JSON.parse(storedCart);\n    }\n    this.filteredProducts = [...this.products];\n  },\n  methods: {\n    getImage(imageName) {\n      return require(`../assets/images/${imageName}`);\n    },\n\n    // Ajouter un produit au panier\n    addToCart(product) {\n      const existingProduct = this.cart.find((item) => item.id === product.id);\n      if (existingProduct) {\n        existingProduct.quantity++;\n      } else {\n        this.cart.push({ ...product, quantity: 1 });\n      }\n      // Sauvegarder le panier dans le localStorage après chaque ajout\n      localStorage.setItem(\"cart\", JSON.stringify(this.cart));\n    },\n\n    // Afficher/Masquer le panier\n    toggleCart() {\n      this.cartVisible = !this.cartVisible;\n    },\n\n    // Filtrage des produits\n    filterProducts() {\n      let filtered = this.products;\n\n      if (this.searchQuery) {\n        filtered = filtered.filter(\n          (product) =>\n            product.name\n              .toLowerCase()\n              .includes(this.searchQuery.toLowerCase()) ||\n            product.category\n              .toLowerCase()\n              .includes(this.searchQuery.toLowerCase())\n        );\n      }\n\n      if (this.selectedCollection) {\n        filtered = filtered.filter(\n          (product) => product.category === this.selectedCollection\n        );\n      }\n\n      if (this.selectedStars) {\n        filtered = filtered.filter(\n          (product) => product.stars === parseInt(this.selectedStars)\n        );\n      }\n\n      if (this.price > 0) {\n        filtered = filtered.filter((product) => product.price <= this.price);\n      }\n\n      this.filteredProducts = filtered;\n    },\n\n    // Trier les produits\n    sortProducts() {\n      if (this.sortOrder === \"asc\") {\n        this.filteredProducts.sort((a, b) => a.price - b.price);\n      } else if (this.sortOrder === \"desc\") {\n        this.filteredProducts.sort((a, b) => b.price - a.price);\n      } else if (this.sortOrder === \"stars\") {\n        this.filteredProducts.sort((a, b) => b.stars - a.stars);\n      }\n    },\n\n    // Méthodes de filtrage par étoiles et collection\n    filterByStars() {\n      this.filterProducts();\n    },\n    filterByCollection() {\n      this.filterProducts();\n    },\n\n    // Méthode pour vider le panier (si nécessaire)\n    clearCart() {\n      this.cart = [];\n      localStorage.removeItem(\"cart\");\n    },\n  },\n};\n</script>\n"],"mappings":";;;;AAmEA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QACEC,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,SAAS;QACfC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,QAAQ;QAClBC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,UAAU;QAChBC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,gBAAgB;QAC1BC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,WAAW;QACrBC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,QAAQ;QACdC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,YAAY;QACtBC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,MAAM;QACZC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,SAAS;QACnBC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,WAAW;QACjBC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,OAAO;QACjBC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE,IAAI;QACXC,QAAQ,EAAE,SAAS;QACnBC,KAAK,EAAE,CAAC;QACRC,KAAK,EAAE;MACT,CAAC,CACF;MACDC,IAAI,EAAE