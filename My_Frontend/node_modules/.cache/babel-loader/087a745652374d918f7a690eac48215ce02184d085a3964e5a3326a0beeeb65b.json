{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.for-each.js\";\nimport header_component from './components/header_component.vue';\nimport footer_component from './components/footer_component.vue';\nexport default {\n  components: {\n    header_component,\n    footer_component\n  },\n  mounted() {\n    // Ajouter des écouteurs d'événements après le montage du composant\n    this.$refs.searchFilter.addEventListener(\"change\", this.handleFilterChange);\n    this.$refs.searchInput.addEventListener(\"input\", this.handleSearchInput);\n  },\n  methods: {\n    handleFilterChange() {\n      const changeOrder = this.$refs.searchFilter.value;\n      if (changeOrder === \"Ascending order\") {\n        this.prices_products(true);\n      } else if (changeOrder === \"Descending order\") {\n        this.prices_products(false);\n      } else if (changeOrder === \"\") {\n        this.reset_order();\n      }\n    },\n    prices_products(ascending) {\n      const allProducts = this.$el.querySelector(\".main_cont\");\n      const products = Array.from(allProducts.getElementsByClassName(\"product\"));\n      products.sort((a, b) => {\n        const price1 = parseFloat(a.getAttribute(\"data-price\"));\n        const price2 = parseFloat(b.getAttribute(\"data-price\"));\n        return ascending ? price1 - price2 : price2 - price1;\n      });\n      products.forEach(product => {\n        allProducts.appendChild(product);\n      });\n    },\n    reset_order() {\n      const allProducts = this.$el.querySelector(\".main_cont\");\n      const products = Array.from(allProducts.getElementsByClassName(\"product\"));\n      products.sort((a, b) => {\n        const index1 = parseInt(a.getAttribute(\"data-index\"));\n        const index2 = parseInt(b.getAttribute(\"data-index\"));\n        return index1 - index2;\n      });\n      products.forEach(product => {\n        allProducts.appendChild(product);\n      });\n    },\n    handleSearchInput() {\n      const searchAll = this.$refs.searchInput.value.toLowerCase();\n      const products = this.$el.querySelectorAll(\".product\");\n      products.forEach(product => {\n        const allNames = product.querySelector(\".info p\").textContent.toLowerCase();\n        product.style.display = allNames.includes(searchAll) ? \"block\" : \"none\";\n      });\n    }\n  }\n};","map":{"version":3,"names":["header_component","footer_component","components","mounted","$refs","searchFilter","addEventListener","handleFilterChange","searchInput","handleSearchInput","methods","changeOrder","value","prices_products","reset_order","ascending","allProducts","$el","querySelector","products","Array","from","getElementsByClassName","sort","a","b","price1","parseFloat","getAttribute","price2","forEach","product","appendChild","index1","parseInt","index2","searchAll","toLowerCase","querySelectorAll","allNames","textContent","style","display","includes"],"sources":["/home/kapopay/Copy_Shop/src/components/product_list.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <header_component />\n    <div>\n      <select ref=\"searchFilter\" id=\"search_filter\">\n        <option value=\"\">Select Order</option>\n        <option value=\"Ascending order\">Ascending order</option>\n        <option value=\"Descending order\">Descending order</option>\n      </select>\n      <input ref=\"searchInput\" id=\"search_input\" type=\"text\" placeholder=\"Search...\" />\n    </div>\n    <div class=\"main_cont\">\n      <!-- Vos produits ici avec la classe \"product\" et les attributs data-price et data-index -->\n    </div>\n    <router-view />\n    <footer_component />\n  </div>\n</template>\n\n<script>\nimport header_component from './components/header_component.vue';\nimport footer_component from './components/footer_component.vue';\n\nexport default {\n  components: {\n    header_component,\n    footer_component,\n  },\n  mounted() {\n    // Ajouter des écouteurs d'événements après le montage du composant\n    this.$refs.searchFilter.addEventListener(\"change\", this.handleFilterChange);\n    this.$refs.searchInput.addEventListener(\"input\", this.handleSearchInput);\n  },\n  methods: {\n    handleFilterChange() {\n      const changeOrder = this.$refs.searchFilter.value;\n      if (changeOrder === \"Ascending order\") {\n        this.prices_products(true);\n      } else if (changeOrder === \"Descending order\") {\n        this.prices_products(false);\n      } else if (changeOrder === \"\") {\n        this.reset_order();\n      }\n    },\n    prices_products(ascending) {\n      const allProducts = this.$el.querySelector(\".main_cont\");\n      const products = Array.from(allProducts.getElementsByClassName(\"product\"));\n\n      products.sort((a, b) => {\n        const price1 = parseFloat(a.getAttribute(\"data-price\"));\n        const price2 = parseFloat(b.getAttribute(\"data-price\"));\n        return ascending ? price1 - price2 : price2 - price1;\n      });\n\n      products.forEach(product => {\n        allProducts.appendChild(product);\n      });\n    },\n    reset_order() {\n      const allProducts = this.$el.querySelector(\".main_cont\");\n      const products = Array.from(allProducts.getElementsByClassName(\"product\"));\n\n      products.sort((a, b) => {\n        const index1 = parseInt(a.getAttribute(\"data-index\"));\n        const index2 = parseInt(b.getAttribute(\"data-index\"));\n        return index1 - index2;\n      });\n\n      products.forEach(product => {\n        allProducts.appendChild(product);\n      });\n    },\n    handleSearchInput() {\n      const searchAll = this.$refs.searchInput.value.toLowerCase();\n      const products = this.$el.querySelectorAll(\".product\");\n\n      products.forEach(product => {\n        const allNames = product.querySelector(\".info p\").textContent.toLowerCase();\n        product.style.display = allNames.includes(searchAll) ? \"block\" : \"none\";\n      });\n    }\n  }\n};\n</script>\n\n<style>\n/* Ajoutez ici vos styles si nécessaire */\n</style>\n"],"mappings":";;AAoBA,OAAOA,gBAAe,MAAO,mCAAmC;AAChE,OAAOC,gBAAe,MAAO,mCAAmC;AAEhE,eAAe;EACbC,UAAU,EAAE;IACVF,gBAAgB;IAChBC;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,kBAAkB,CAAC;IAC3E,IAAI,CAACH,KAAK,CAACI,WAAW,CAACF,gBAAgB,CAAC,OAAO,EAAE,IAAI,CAACG,iBAAiB,CAAC;EAC1E,CAAC;EACDC,OAAO,EAAE;IACPH,kBAAkBA,CAAA,EAAG;MACnB,MAAMI,WAAU,GAAI,IAAI,CAACP,KAAK,CAACC,YAAY,CAACO,KAAK;MACjD,IAAID,WAAU,KAAM,iBAAiB,EAAE;QACrC,IAAI,CAACE,eAAe,CAAC,IAAI,CAAC;MAC5B,OAAO,IAAIF,WAAU,KAAM,kBAAkB,EAAE;QAC7C,IAAI,CAACE,eAAe,CAAC,KAAK,CAAC;MAC7B,OAAO,IAAIF,WAAU,KAAM,EAAE,EAAE;QAC7B,IAAI,CAACG,WAAW,CAAC,CAAC;MACpB;IACF,CAAC;IACDD,eAAeA,CAACE,SAAS,EAAE;MACzB,MAAMC,WAAU,GAAI,IAAI,CAACC,GAAG,CAACC,aAAa,CAAC,YAAY,CAAC;MACxD,MAAMC,QAAO,GAAIC,KAAK,CAACC,IAAI,CAACL,WAAW,CAACM,sBAAsB,CAAC,SAAS,CAAC,CAAC;MAE1EH,QAAQ,CAACI,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACtB,MAAMC,MAAK,GAAIC,UAAU,CAACH,CAAC,CAACI,YAAY,CAAC,YAAY,CAAC,CAAC;QACvD,MAAMC,MAAK,GAAIF,UAAU,CAACF,CAAC,CAACG,YAAY,CAAC,YAAY,CAAC,CAAC;QACvD,OAAOb,SAAQ,GAAIW,MAAK,GAAIG,MAAK,GAAIA,MAAK,GAAIH,MAAM;MACtD,CAAC,CAAC;MAEFP,QAAQ,CAACW,OAAO,CAACC,OAAM,IAAK;QAC1Bf,WAAW,CAACgB,WAAW,CAACD,OAAO,CAAC;MAClC,CAAC,CAAC;IACJ,CAAC;IACDjB,WAAWA,CAAA,EAAG;MACZ,MAAME,WAAU,GAAI,IAAI,CAACC,GAAG,CAACC,aAAa,CAAC,YAAY,CAAC;MACxD,MAAMC,QAAO,GAAIC,KAAK,CAACC,IAAI,CAACL,WAAW,CAACM,sBAAsB,CAAC,SAAS,CAAC,CAAC;MAE1EH,QAAQ,CAACI,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACtB,MAAMQ,MAAK,GAAIC,QAAQ,CAACV,CAAC,CAACI,YAAY,CAAC,YAAY,CAAC,CAAC;QACrD,MAAMO,MAAK,GAAID,QAAQ,CAACT,CAAC,CAACG,YAAY,CAAC,YAAY,CAAC,CAAC;QACrD,OAAOK,MAAK,GAAIE,MAAM;MACxB,CAAC,CAAC;MAEFhB,QAAQ,CAACW,OAAO,CAACC,OAAM,IAAK;QAC1Bf,WAAW,CAACgB,WAAW,CAACD,OAAO,CAAC;MAClC,CAAC,CAAC;IACJ,CAAC;IACDtB,iBAAiBA,CAAA,EAAG;MAClB,MAAM2B,SAAQ,GAAI,IAAI,CAAChC,KAAK,CAACI,WAAW,CAACI,KAAK,CAACyB,WAAW,CAAC,CAAC;MAC5D,MAAMlB,QAAO,GAAI,IAAI,CAACF,GAAG,CAACqB,gBAAgB,CAAC,UAAU,CAAC;MAEtDnB,QAAQ,CAA